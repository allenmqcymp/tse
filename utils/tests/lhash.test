#!/bin/bash

# Allen Ma, driver code for testing lhash
# dependency - runtest.sh placed in ~/bin

for f in test_lhash*
do
    # if it ends in .o or .c, then don't test it
    if [ ${f: -2 } != ".c" ] && [ ${f: -2} != ".o" ]; then
        echo "running ./${f}"
        runtest.sh "./${f}"
    fi 
done